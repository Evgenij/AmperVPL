//---------------------------------------------------------------------------
#pragma package(smart_init)
#pragma resource "*.dfm"
TFMenu *FMenu;
//---------------------------------------------------------------------------
__fastcall TFMenu::TFMenu(TComponent* Owner)
	: TForm(Owner)
{
}
//---------------------------------------------------------------------------
void __fastcall TFMenu::FormActivate(TObject *Sender)
{
	Label1->Font->Color=0x00FF5F00;
	Label2->Font->Color=0x00FF5F00;
	Label3->Font->Color=0x00FF5F00;
	Label4->Font->Color=0x00FF5F00;
	Label5->Font->Color=0x00FF5F00;
	Label6->Font->Color=0x00FF5F00;
	Label7->Font->Color=0x00FF5F00;
	Load->ShowModal();
}
//---------------------------------------------------------------------------
void __fastcall TFMenu::Label3MouseMove(TObject *Sender, TShiftState Shift, int X,
          int Y)
{
	Label3->Font->Color=0x00FFD3B2;
}
//---------------------------------------------------------------------------

void __fastcall TFMenu::Label3MouseLeave(TObject *Sender)
{
	Label3->Font->Color=0x00FF5F00;
}
//---------------------------------------------------------------------------

void __fastcall TFMenu::Label4MouseMove(TObject *Sender, TShiftState Shift, int X,
          int Y)
{
	Label4->Font->Color=0x00FFD3B2;
}
//---------------------------------------------------------------------------

void __fastcall TFMenu::Label4MouseLeave(TObject *Sender)
{
	Label4->Font->Color=0x00FF5F00;
}
//---------------------------------------------------------------------------

void __fastcall TFMenu::Label5MouseMove(TObject *Sender, TShiftState Shift, int X,
          int Y)
{
	Label5->Font->Color=0x00FFD3B2;
}
//---------------------------------------------------------------------------

void __fastcall TFMenu::Label5MouseLeave(TObject *Sender)
{
	Label5->Font->Color=0x00FF5F00;
}
//---------------------------------------------------------------------------

void __fastcall TFMenu::Label6MouseMove(TObject *Sender, TShiftState Shift, int X,
          int Y)
{
    Label5->Font->Color=0x00FFD3B2;
}
